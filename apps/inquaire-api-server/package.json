{
  "name": "@ai-next/inquaire-api-server",
  "version": "0.1.0",
  "description": "AI 상담 매니저 - Backend API Server",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "node -e \"const fs=require('fs');const path=require('path');const src='../src/prisma';const dest=path.join('node_modules','@prisma','generated');fs.mkdirSync(path.dirname(dest),{recursive:true});try{fs.unlinkSync(dest)}catch(e){};fs.symlinkSync(path.resolve(src),dest,'junction');\"",
    "type-check": "tsc --noEmit",
    "build": "pnpm prisma:generate && nest build && tsc-alias -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_OPTIONS='-r tsconfig-paths/register' nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "cross-env NODE_ENV=test dotenv -e envs/.env.e2e -- jest --config ./test/jest-e2e.json --testPathPattern=e2e",
    "test:integration": "cross-env NODE_ENV=test dotenv -e envs/.env.integration -- jest --config ./test/jest-integration.json",
    "test:performance": "cross-env NODE_ENV=test dotenv -e envs/.env.performance -- jest --config ./test/jest-performance.json",
    "test:e2e:watch": "cross-env NODE_ENV=test dotenv -e envs/.env.e2e -- jest --config ./test/jest-e2e.json --watch",
    "test:integration:watch": "cross-env NODE_ENV=test dotenv -e envs/.env.integration -- jest --config ./test/jest-integration.json --watch",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma && echo \"export * from './client';\" > ./prisma/generated/index.ts",
    "prisma:migrate": "prisma migrate dev --schema=./prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema=./prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./prisma/schema.prisma",
    "prisma:seed": "tsx ./prisma/seed/index.ts",
    "openapi:generate": "ts-node -r tsconfig-paths/register scripts/generate-openapi.ts",
    "seed": "ts-node -r tsconfig-paths/register src/database/seeds/main.seed.ts",
    "generate:encryption-key": "tsx scripts/generate-encryption-key.ts",
    "migrate:encrypt-tokens": "tsx scripts/migrate-encrypt-tokens.ts",
    "migrate:encrypt-tokens:dry-run": "tsx scripts/migrate-encrypt-tokens.ts --dry-run",
    "rotate:encryption-key": "tsx scripts/rotate-encryption-key.ts",
    "rotate:encryption-key:dry-run": "tsx scripts/rotate-encryption-key.ts --dry-run",
    "docker:up": "cd infra/docker && docker-compose up -d",
    "docker:down": "cd infra/docker && docker-compose down",
    "docker:logs": "cd infra/docker && docker-compose logs -f",
    "docker:ps": "cd infra/docker && docker-compose ps",
    "docker:clean": "cd infra/docker && docker-compose down -v",
    "docker:tools": "cd infra/docker && docker-compose --profile tools up -d",
    "db": "tsx infra/scripts/db-manager.ts",
    "db:setup": "tsx infra/scripts/db-manager.ts setup",
    "db:start": "tsx infra/scripts/db-manager.ts start",
    "db:stop": "tsx infra/scripts/db-manager.ts stop",
    "db:status": "tsx infra/scripts/db-manager.ts status",
    "db:migrate": "tsx infra/scripts/db-manager.ts migrate:dev",
    "db:migrate:test": "tsx infra/scripts/db-manager.ts migrate:test",
    "db:migrate:all": "tsx infra/scripts/db-manager.ts migrate:all",
    "db:reset": "tsx infra/scripts/db-manager.ts reset:dev",
    "db:reset:test": "tsx infra/scripts/db-manager.ts reset:test",
    "db:seed": "tsx infra/scripts/db-manager.ts seed",
    "db:studio": "tsx infra/scripts/db-manager.ts studio:dev",
    "db:studio:test": "tsx infra/scripts/db-manager.ts studio:test",
    "test:setup": "tsx infra/scripts/test-setup.ts setup",
    "test:migrate": "tsx infra/scripts/test-setup.ts migrate",
    "test:migrate:e2e": "tsx infra/scripts/test-setup.ts migrate e2e",
    "test:migrate:integration": "tsx infra/scripts/test-setup.ts migrate integration",
    "test:migrate:performance": "tsx infra/scripts/test-setup.ts migrate performance",
    "test:reset:e2e": "tsx infra/scripts/test-setup.ts reset e2e",
    "test:reset:integration": "tsx infra/scripts/test-setup.ts reset integration",
    "test:reset:performance": "tsx infra/scripts/test-setup.ts reset performance",
    "test:cleanup:e2e": "tsx infra/scripts/test-setup.ts cleanup e2e",
    "test:cleanup:integration": "tsx infra/scripts/test-setup.ts cleanup integration",
    "test:seed:e2e": "tsx infra/scripts/test-setup.ts seed e2e",
    "test:seed:integration": "tsx infra/scripts/test-setup.ts seed integration",
    "test:status": "tsx infra/scripts/test-setup.ts status",
    "dev:setup": "pnpm db:setup && pnpm db:seed",
    "dev:fresh": "pnpm db:reset && pnpm db:seed && pnpm start:dev"
  },
  "dependencies": {
    "@ai-next/nestjs-shared": "workspace:*",
    "@aws-sdk/client-ses": "^3.940.0",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/throttler": "^6.2.1",
    "@prisma/adapter-pg": "^7.0.0",
    "@prisma/client": "^7.0.0",
    "@sentry/node": "^10.25.0",
    "@sentry/profiling-node": "^10.25.0",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.63.2",
    "cache-manager-ioredis-yet": "^2.1.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.1",
    "handlebars": "^4.7.8",
    "helmet": "^8.0.0",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.32.0",
    "joi": "^18.0.1",
    "nest-winston": "^1.10.2",
    "nodemailer": "^7.0.11",
    "openai": "^4.73.0",
    "opossum": "^9.0.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@prisma/client-runtime-utils": "^7.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^7.0.4",
    "@types/opossum": "^8.1.9",
    "@types/passport-jwt": "^4.0.0",
    "@types/pg": "^8.15.5",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.7",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2",
    "prisma": "^7.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(isomorphic-dompurify|dompurify|jsdom|parse5)/)"
    ]
  }
}
